{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "es2015.promise", "es2015.collection", "dom"],
		"strict": true,
		"strictPropertyInitialization": false,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules"
	],
	"filesGlob": [
		"src/js/*.ts"
	]
}
