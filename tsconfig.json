{
	"compileOnSave": false,
	"compilerOptions": {
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"skipLibCheck": true,
		"target": "es5",
        "lib": [
            "es5", "es2015.promise", "es2015.collection", "dom"
        ]
	},
	"exclude": [
		"node_modules"
	],
	"filesGlob": [
		"src/js/*.ts"
	]
}
